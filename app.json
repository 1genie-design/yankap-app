{
  "expo": {
    "name": "Yankap",
    "slug": "yankap-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "backgroundColor": "#1DBAA3"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "yankap",
    "plugins": ["expo-router"]
  }
}